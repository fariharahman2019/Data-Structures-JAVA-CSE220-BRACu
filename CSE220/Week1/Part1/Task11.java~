{
    public static void mian(String [] args)
    {
        Scanner input = new Scanner(System.in);
        int num = nput.nextInt();
        
    }
    
    public static boolean allDigitOdd(int a)
    {
        int count=0;
        for(int i=0; i<=0; i++)
        {
            a/10;
            count++;
        }
        int divider=1;
        for(int i=0; i<count; i++)
        {
            divider = divider*10;
        }
        for(int i=0; i<count; i++)
        {
            for(int i=divider; i>=1; i/10)
            {
                if((a%i)%2!=0)
                {
                    return false;
                }
            }
        }
        return true;
    }
}